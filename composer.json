{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "ercogx/laravel-filament-starter-kit",
    "type": "project",
    "description": "This is a Filament v3 Starter Kit for Laravel 12, designed to accelerate the development of Filament-powered applications.",
    "keywords": [
        "laravel",
        "framework",
        "glow starter kit",
        "livewire",
        "filament"
    ],
    "license": "MIT",

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/3pontos-tech/filament-consultants-module"
        }
    ],
    "require": {
        "php": "^8.3",
        "3pontos-tech/filament-consultants-module": "dev-main",
        "basementdevs/abacatepay-php-sdk": "^1.0",
        "bezhansalleh/filament-shield": "^4.0",
        "blade-ui-kit/blade-icons": "^1.8",
        "danharrin/livewire-rate-limiting": "^2.1",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "guava/filament-icon-picker": "^3.0",
        "jeffgreco13/filament-breezy": "^3.0",
        "laravel/framework": "^12.0",
        "laravel/nightwatch": "^1.12",
        "laravel/tinker": "^2.10.1",
        "mallardduck/blade-lucide-icons": "^1.23",
        "outerweb/filament-settings": "^2.0",
        "owenvoke/blade-fontawesome": "^2.9",
        "pxlrbt/filament-environment-indicator": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "driftingly/rector-laravel": "^2.0.4",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^v4.0",
        "pestphp/pest-plugin-laravel": "^v4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^v4.0",
        "rector/rector": "^2.0.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "pint": "vendor/bin/pint",
        "test:pint": "vendor/bin/pint --test",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "test:phpstan": "vendor/bin/phpstan analyse --ansi",
        "rector": "vendor/bin/rector",
        "test:rector": "vendor/bin/rector --dry-run",
        "check": [
            "@test:rector",
            "@test:lint"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test --coverage"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
